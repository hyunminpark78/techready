  "private": true,
  "scripts": {
    "dev": "cross-env BUILD_ENV=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js ",
    "stg": "cross-env BUILD_ENV=stg webpack-dev-server --inline --progress --config build/webpack.dev.conf.js ",
    "prd": "cross-env BUILD_ENV=prd webpack-dev-server --inline --progress --config build/webpack.dev.conf.js ",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src",
    "build": "npm run build.dev",
    "build.dev": "cross-env BUILD_ENV=dev node --max-old-space-size=2048 build/build.js",
    "build.stg": "cross-env BUILD_ENV=stg node --max-old-space-size=2048 build/build.js",
    "build.prd": "cross-env BUILD_ENV=prd node --max-old-space-size=2048 build/build.js",
    "sonar": "sonar-scanner -Dsonar.sources=. -Dsonar.host.url=SONARQUBE -Dsonar.login=SONAR_TOKEN",
    "svg": "vsvg -s ./static/svg/icon -t ./src/components/icons",
    "prettier": "prettier '**/*.{js,ts,vue}'",
    "postinstall": "patch-package"
  },
